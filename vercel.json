{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next",
        "config": {
          "nodeVersion": "18.x"
        }
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production",
        "NEXT_PUBLIC_VERCEL_ENV": "production"
      },
      "timeout": 60
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ],
    "functions": {
      "api/**": {
        "memory": 3008,
        "maxDuration": 30
      }
    },
    "framework": "nextjs",
    "installCommand": "npm install --legacy-peer-deps",
    "outputDirectory": ".next"
  }